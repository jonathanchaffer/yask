{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "Template to help kickstart new projects",
  "scripts": {
    "build": "run-s build:*",
    "build:typescript": "tsc && tsc-alias",
    "start": "run-p start:*",
    "start:server": "node dist/server/index.js",
    "dev": "run-p dev:*",
    "dev:server": "npm run build; tsc -w & tsc-alias -w & nodemon dist/server/index.js",
    "test": "run-s test:*",
    "test:unit": "vitest",
    "check": "run-s check:*",
    "check:lint": "eslint .",
    "check:type": "tsc --noEmit",
    "check:format": "prettier --check .",
    "fix": "run-s fix:*",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier --write .",
    "clean": "rm -rf dist && rm -rf node_modules",
    "migrate": "run-s migrate:*",
    "migrate:reset": "run-s migrate:reset:*",
    "migrate:reset:drizzle": "ts-node -T src/db/drizzle/reset.ts",
    "migrate:create": "run-s migrate:create:*",
    "migrate:create:drizzle": "drizzle-kit generate --config src/db/drizzle/config.ts",
    "migrate:up": "run-s migrate:up:*",
    "migrate:up:drizzle": "drizzle-kit migrate --config src/db/drizzle/config.ts",
    "prepare": "husky"
  },
  "author": "Jonathan Chaffer",
  "license": "ISC",
  "dependencies": {
    "drizzle-orm": "^0.33.0",
    "express": "^4.17.1",
    "postgres": "^3.4.4",
    "redis": "^4.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.1",
    "@types/node": "^22.1.0",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
