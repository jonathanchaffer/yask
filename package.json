{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "Template to help kickstart new projects",
  "scripts": {
    "build": "run-s build:*",
    "build:typescript": "tsc && tsc-alias",
    "start": "run-p start:*",
    "start:server": "node dist/server/index.js",
    "dev": "run-p dev:*",
    "dev:server": "npm run build; tsc -w & tsc-alias -w & nodemon dist/server/index.js",
    "test": "run-s test:*",
    "test:unit": "vitest",
    "lint": "eslint .",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "author": "Jonathan Chaffer",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "redis": "^4.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/express": "^4.17.1",
    "@types/node": "^22.1.0",
    "eslint": "^8",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vitest": "^2.0.5"
  }
}
